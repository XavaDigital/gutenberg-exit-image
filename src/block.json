{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "xavadigital/exit-image",
	"version": "0.1.0",
	"title": "Exit Image",
	"category": "widgets",
	"icon": "format-image",
	"description": "An image block with random exit animation on click",
	"supports": {
		"html": false
	},
	"textdomain": "exit-image",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"viewScript": "file:./js/script.js",
	"style": "file:./style-index.css",
	"attributes": {
		"imageId": {
			"type": "number",
			"default": 0
		},
		"imageURL": {
			"type": "string",
			"default": ""
		}
	}
}
